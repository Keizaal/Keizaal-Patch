{
  "$schema": "https://raw.githubusercontent.com/Exit-9B/InventoryInjector/main/docs/InventoryInjector.schema.json",
  "rules": [
    {
      "match": {
        "formType": "Weapon",
        "keywords": [ "WeapTypeSpear" ]
      },
      "assign": {
        "subTypeDisplay": "$Spear",
        "iconSource": "skyui_weapons_pack/icons.swf",
        "iconLabel": "weapon_spear"
      }
    },
